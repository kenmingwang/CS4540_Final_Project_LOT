                        <th scope="col">Course</th>
                        <th scope="col">
                            Professor
                        </th>
                        <th scope="col">
                            Progress
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (KeyValuePair<Course,string> courseMap in @ViewData["CoursesMap"] as IEnumerable<KeyValuePair<Course,string>>)
                    {
                         var name = UserNameAndRolesUtil.UserNameToActualName(courseMap.Value);
                        <tr>
                            <td class="td-width">
                                <a asp-action="Details" asp-route-cId="@courseMap.Key.CId">
