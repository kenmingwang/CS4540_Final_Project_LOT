@using CS4540_A2.Util

@{ ViewData["Title"] = "Deparment Chair"; }

<script type="text/javascript">
    function ramdomProgress() {
        var x = document.getElementsByClassName('progress-bar');
        var i;
        for (i = 0; i < x.length; i++) {
            x.item(i).setAttribute('style', 'width:' + Math.random() * 100 + '%');
        }

    }
</script>

